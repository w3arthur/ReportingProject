  <DataSets>
    <DataSet Name="dtsFilms">
      <Query>
        <DataSourceName>dscMovies</DataSourceName>
        <CommandText>SELECT        tblFilm.FilmName, tblStudio.StudioName, tblFilm.FilmReleaseDate, tblFilm.FilmBudgetDollars
FROM            tblFilm INNER JOIN
                         tblStudio ON tblFilm.FilmStudioID = tblStudio.StudioID</CommandText>
      </Query>
