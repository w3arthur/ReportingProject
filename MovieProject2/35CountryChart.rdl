  <DataSets>
    <DataSet Name="stcCountryFilms">
      <Query>
        <DataSourceName>dscMovies</DataSourceName>
        <CommandText>SELECT        tblCountry.CountryID, tblCountry.CountryName, tblFilm.FilmBoxOfficeDollars
FROM            tblCountry INNER JOIN
                         tblFilm ON tblCountry.CountryID = tblFilm.FilmCountryID</CommandText>
      </Query>
